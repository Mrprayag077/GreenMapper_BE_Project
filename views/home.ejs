                    }

                    .follow-creator img {
                        width: 40px;
                        height: 40px;
                        margin-right: 10px;
                    }

                    .follow-creator h3 {
                        font-size: 13px;
                        color: #13131d;
                    }

                    .follow-creator p {
                        font-size: 12px;
                    }

                    .following {
                        font-size: 12px;
                        font-weight: 400;
                        padding: 8px 15px;
                        margin-bottom: 18px;
                    }

                    .follow {
                        background: #fdfdfd;
                        color: #13131d;
                        border: 1px solid rgba(161, 162, 175, 0.3);
