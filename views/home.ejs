                                                // Remove the "blinking-background" class after the animation is completed
                                                sloganBtn.addEventListener('animationend', function () {
                                                    sloganBtn.classList.remove('blinking-background');
                                                }, { once: true }); // Use the "once" option to ensure the event listener is executed only once
                                            }, 2000);


                                            // Array of slogan
