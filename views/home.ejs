                                    <script>
                                        // Sample data for tree count over time
                                        const treeData = [
                                            { year: 2010, count: 100 },
                                            { year: 2011, count: 105 },
                                            { year: 2012, count: 90 },
                                            { year: 2013, count: 110 },
                                            { year: 2014, count: 130 },
                                            { year: 2015, count: 133 },
                                            { year: 2016, count: 128 },
                                            { year: 2017, count: 115 },
                                            { year: 2018, count: 90 },
                                            { year: 2019, count: 120 }
                                        ];

                                        // Get the canvas element and context
                                        const canvas = document.getElementById('treeChart');
                                        const ctx = canvas.getContext('2d');

                                        // Extract years and counts for labels and data points
                                        const years = treeData.map(entry => entry.year);
                                        const counts = treeData.map(entry => entry.count);

                                        // Determine point colors based on count decrease
                                        const pointColors = counts.map((count, index) => {
                                            if (index > 0 && count < counts[index - 1]) {
                                                return 'red'; // Mark points in red when count decreases
                                            } else {
                                                return '#5dcb67'; // Use green for other points
                                            }
                                        });

                                        // Create an area chart
                                        const treeChart = new Chart(ctx, {
                                            type: 'line',
                                            data: {
                                                labels: years,
                                                datasets: [{
                                                    data: counts,
                                                    borderColor: '#5dcb67',
                                                    backgroundColor: '#e6f7e8', // Light green fill color
                                                    fill: true, // Fill the area under the line
                                                    borderWidth: 2,
                                                    pointRadius: 4, // Adjust the radius of the points
                                                    pointBackgroundColor: pointColors, // Array of point colors
                                                    pointBorderWidth: 2, // Point border width
                                                }]
                                            },
                                            options: {
                                                responsive: true,
                                                maintainAspectRatio: false,
                                                animation: {
                                                    duration: 2000, // 2000 milliseconds (2 seconds) animation
                                                    easing: 'easeInOutQuart' // Easing function for smooth animation
                                                },
                                                scales: {
                                                    x: {
                                                        display: false // Hide x-axis
                                                    },
                                                    y: {
                                                        display: false // Hide y-axis
                                                    }
                                                },
                                                plugins: {
                                                    legend: {
                                                        display: false // Hide legend
                                                    }
                                                }
                                            }
                                        });
                                    </script>

