                    .banner {
                        width: 100%;
                        height: 210px;
                        border-radius: 18px;
                        background: #55c960 url(https://raw.githubusercontent.com/programmercloud/nft-dashboard/main/img/bannerBG.svg);
                        background-size: cover;
                        background-repeat: no-repeat;
                        background-position: center;
                        transition: 0.3s ease;
                        padding: 20px;
                        position: relative;
                        color: #fff;
                    }

                    .banner h2 {
                        font-size: 28px;
                        font-weight: 700;
                    }

                    .banner img {
                        width: 420px;
                        position: absolute;
                        bottom: 0;
                        right: -20px;
                        pointer-events: none;
                    }

                    .btn {
                        padding: 14px 16px;
                        text-decoration: none;
                        display: inline-block;
                        background: #161622;
                        color: #fff;
                        border-radius: 25px;
                        font-size: 18px;
                        font-weight: 400;
                        margin-top: 20px;
                        transition: 0.4s linear;
                    }

                    .blinking-background {
                        animation: blinkBackground 3s forwards;
                        /* Play animation once and stay at the final keyframe */
                        transition: background 1s linear;
                    }

                    @keyframes blinkBackground {
                        0% {
                            background: #161622;
                        }

                        100% {
                            background: #ffd700;
                            color: #161622;
                            font-weight: 900;
                            /* Change this to your desired blink color */
                        }
                    }


                    .stay-red {
                        padding: 14px 16px;
                        text-decoration: none;
                        display: inline-block;
                        background: red;
                        color: #fff;
                        border-radius: 25px;
                        font-size: 18px;
                        font-weight: 400;
                        margin-top: 20px;
                        transition: 0.4s linear;

